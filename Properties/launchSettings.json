{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "/ui",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy",
        "ASPNETCORE_SPA_LAUNCH": "TRUE"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:9255"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "/ui",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy",
        "ASPNETCORE_SPA_LAUNCH": "TRUE"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:9256;http://localhost:9255"
    },
    "http (no spa)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "/ui",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_SPA_LAUNCH": "FALSE"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:9255"
    },
    "https (no spa)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "/ui",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_SPA_LAUNCH": "FALSE"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:9256;http://localhost:9255"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}